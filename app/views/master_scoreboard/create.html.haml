= debug@game_data[0]
- number = 0
-
-while @game_data[number] do
  -if @game_data[number][:status] == 'Postponed'
    =puts "Postponed"
  -else
  %br
  %br
    %table.table.table-bordered
      %tr
        %th
        %th 1
        %th 2
        %th 3
        %th 4
        %th 5
        %th 6
        %th 7
        %th 8
        %th 9
        %th R
        %th H
        %th E
      %tbody
        %tr
          %td= @game_data[number][:away_team_name]
          %td= @game_data[number][:score][1][:away]
          %td= @game_data[number][:score][2][:away]
          %td= @game_data[number][:score][3][:away]
          %td= @game_data[number][:score][4][:away]
          %td= @game_data[number][:score][5][:away]
          %td= @game_data[number][:score][6][:away]
          %td= @game_data[number][:score][7][:away]
          %td= @game_data[number][:score][8][:away]
          %td= @game_data[number][:score][9][:away]
          -if @game_data[number][:away_run]
            %td= @game_data[number][:away_run]
          -if @game_data[number][:away_hit]
            %td= @game_data[number][:away_hit]
          -if @game_data[number][:away_er]
            %td= @game_data[number][:away_er]

        %tr
          %td= @game_data[number][:home_team_name]
          %td= @game_data[number][:score][1][:home]
          %td= @game_data[number][:score][2][:home]
          %td= @game_data[number][:score][3][:home]
          %td= @game_data[number][:score][4][:home]
          %td= @game_data[number][:score][5][:home]
          %td= @game_data[number][:score][6][:home]
          %td= @game_data[number][:score][7][:home]
          %td= @game_data[number][:score][8][:home]
          %td= @game_data[number][:score][9][:home]
          -if @game_data[number][:home_run]
            %td= @game_data[number][:home_run]
          -if @game_data[number][:home_hit]
            %td= @game_data[number][:home_hit]
          -if @game_data[number][:home_er]
            %td= @game_data[number][:home_er]
    %span.label.label-success 勝ち
    = @game_data[number][:winning_pitcher].to_s + "(#{@game_data[number][:winning_pitcher_win]}-#{@game_data[number][:winning_pitcher_lose]})" + ' ' + @game_data[number][:winning_pitcher_era].to_s
    %span.label.label-important 負け
    = @game_data[number][:losing_pitcher].to_s + "(#{@game_data[number][:losing_pitcher_win]}-#{@game_data[number][:losing_pitcher_lose]})" + ' ' + @game_data[number][:losing_pitcher_era].to_s
    -if @game_data[number][:save_pitcher_svo] != '0'
      %span.label セーブ
      = @game_data[number][:save_pitcher].to_s + "(#{@game_data[number][:save_pitcher_win]}-#{@game_data[number][:save_pitcher_lose]})" + ' ' + @game_data[number][:save_pitcher_save] + '/' +@game_data[number][:save_pitcher_svo] + ' ' + @game_data[number][:save_pitcher_era].to_s
    -else
  %br
  .alert.alert-block
    %p= "#{@game_data[number][:away_team_name]}:" + "盗塁 =>" + @game_data[number][:away_sb] + ",奪三振 =>" + @game_data[number][:away_so]
    %p= "#{@game_data[number][:home_team_name]}:" + "盗塁 =>" + @game_data[number][:home_sb] + ",奪三振 =>" + @game_data[number][:home_so]
  -hr_player = 0
  -while @game_data[number][hr_player]
    .alert.alert-info= "ホームラン:" + @game_data[number][hr_player][:hr_player_name] + " " + @game_data[number][hr_player][:hr_player_hr_number] + "本 " + @game_data[number][hr_player][:hr_player_hr_inning] + '回'
    - hr_player += 1
  - number += 1


